{
  "name": "biketag-admin",
  "version": "3.0.0",
  "license": "AGPL-3.0-or-later",
  "author": "Ken Eucker",
  "bugs": {
    "url": "https://github.com/keneucker/biketag-admin/issues"
  },
  "scripts": {
    "lint": "npm run lint:js && npm run lint:style",
    "lint:js": "eslint --ext .ts,.js,.vue src --fix",
    "lint:style": "stylelint \"./src/**/*.vue\" \"./src/**/*.scss\" --fix",
    "serve": "vite",
    "pub:expose": "mv dist/index.d.ts ./index.d.ts && mv dist/biketag-admin.* ./ && mv dist/components ./components && mv dist/common ./common && mv dist/store ./store && mv dist/directives ./directives",
    "pub:clear": "rm -f ./index.d.ts ./biketag-admin.es.js ./biketag-admin.css && rm -rf dist ./components ./common ./store",
    "pub:build": "vue-tsc && vite build --config publish.config.ts",
    "build": "vite build",
    "commit": "cz",
    "docs:generate": "npx vuepress-jsdoc -c jsdoc.json --source ./src --dist ./docs --folder api --title Api ",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "dev:rerun": "npm run dev:clear && npm run dev",
    "dev:clear": "rm -rf node_modules/.vite && rm -rf dist",
    "dev": "netlify dev --functionsPort 7200",
    "preview": "npm run build -w && npm run serve dist"
  },
  "dependencies": {
    "@auth0/auth0-vue": "^2.0.0",
    "@img-comparison-slider/vue": "^7.4.3",
    "@serverless-jwt/jwt-verifier": "^0.2.1",
    "@turf/turf": "^6.5.0",
    "ajv": "^8.10.0",
    "autoprefixer": "^10.4.4",
    "biketag": "^3.0.0",
    "crypto-js": "^4.1.1",
    "exifr": "^7.1.3",
    "jose": "^4.6.0",
    "liquidjs": "^9.34.0",
    "loglevel": "^1.8.0",
    "lzutf8": "^0.6.1",
    "md5": "^2.3.0",
    "mitt": "^3.0.0",
    "pinia": "^2.0.0",
    "qs": "^6.10.3",
    "vue": "^3.0.0",
    "vue3-cookies": "^1.0.6"
  },
  "peerDependencies": {
    "biketag": "^3.0.0",
    "pinia": "^2.0.0",
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.23.4",
    "@babel/core": "^7.23.3",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/preset-env": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@commitlint/cli": "^16.1.0",
    "@commitlint/config-conventional": "^16.0.0",
    "@originjs/vite-plugin-commonjs": "^1.0.3",
    "@types/crypto-js": "^4.1.3",
    "@types/md5": "^2.3.4",
    "@types/node": "^20.8.10",
    "@types/qs": "^6.9.11",
    "@types/request": "^2.48.12",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "@vitejs/plugin-vue": "^4.4.0",
    "@vitejs/plugin-vue-jsx": "^3.0.2",
    "@vue/eslint-config-standard": "^8.0.1",
    "@vue/eslint-config-typescript": "^12.0.0",
    "commitizen": "^4.2.4",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-formatter-friendly": "^7.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-vue": "^9.18.1",
    "jsdoc-babel": "^0.5.0",
    "jsdoc-to-markdown": "^8.0.0",
    "netlify-cli": "^16.9.3",
    "path": "^0.12.7",
    "postcss": "^8.4.31",
    "postcss-html": "^1.5.0",
    "prettier": "^3.0.3",
    "sass": "^1.69.5",
    "stylelint": "^15.11.0",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-config-standard-scss": "^11.0.0",
    "stylelint-order": "^6.0.3",
    "stylelint-selector-bem-pattern": "^3.0.1",
    "typescript": "^5.2.2",
    "vite": "^4.5.0",
    "vite-plugin-dts": "^2.3.0",
    "vite-plugin-env-compatible": "^1.1.1",
    "vite-plugin-environment": "^1.1.3",
    "vue-tsc": "^1.4.2",
    "vuepress": "^2.0.0-rc.0"
  },
  "engines": {
    "node": ">=18"
  },
  "lint-staged": {
    "*.ts": [
      "eslint . --fix"
    ],
    "*.{js,css,md,yml,yaml,json}": "prettier . --write"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "keywords": [
    "typescript",
    "biketag",
    "vue3",
    "vue",
    "vuejs"
  ],
  "types": "./index.d.ts",
  "module": "./biketag-admin.es.js",
  "exports": {
    ".": {
      "import": "./biketag-admin.es.js",
      "require": "./biketag-admin.umd.js",
      "types": "./index.d.ts"
    },
    "./biketag-admin.css": {
      "import": "./biketag-admin.css",
      "require": "./biketag-admin.css"
    }
  }
}
